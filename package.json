{
  "name": "browsochrones",
  "version": "0.0.1",
  "description": "",
  "main": "./build/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/browsochrones"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "browserify index.js > build/index.js",
    "develop": "watchify -v -s Static index.js -o build/index.js",
    "docs": "dox -r < index.js | doxme --readme > README.md",
    "prepublish": "npm run-script build",
    "start": "npm run-script develop & serve",
    "test": "standard"
  },
  "author": "Conveyal",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "browser-request": "^0.3.3",
    "buffer": "^3.5.1",
    "geobuf": "^1.0.1",
    "geojson-extent": "^0.3.1",
    "isomorphic-fetch": "^2.2.0",
    "pbf": "^1.3.5",
    "request": "^2.65.0",
    "turf-inside": "^1.1.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "dox": "^0.8.0",
    "doxme": "^1.8.2",
    "leaflet": "^0.7.3",
    "serve": "^1.4.0",
    "standard": "^5.3.1",
    "watchify": "^3.2.0"
  }
}
