    "pbf": "^1.3.5",
    "turf-inside": "^1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-eslint": "^4.1.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "isomorphic-fetch": "^2.2.0",
    "leaflet": "^0.7.3",
    "serve": "^1.4.0",
    "standard": "^5.3.1",
