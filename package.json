{
  "name": "browsochrones",
  "version": "0.0.1",
  "description": "",
  "main": "./build/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/browsochrones"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "browserify": {
    "transform": "babelify"
