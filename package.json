{
  "name": "browsochrones",
  "version": "0.2.2",
  "description": "Create isochrones and accessibility images in the browser",
  "main": "./build/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/browsochrones"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime"
    ]
  },
  "browserify": {
    "transform": "babelify"
  },
  "scripts": {
    "lint": "standard",
    "prepublish": "babel lib --out-dir build",
    "start": "budo example.js --live --verbose",
    "test": "babel-node test/index.js"
  },
  "author": "Conveyal",
  "license": "MIT",
  "dependencies": {
    "color": "^0.10.1",
    "debug": "^2.2.0",
    "jsolines": "0.1.1",
    "json": "^9.0.3",
    "lodash.fill": "^3.3.2",
    "lodash.slice": "^4.0.2",
    "web-worker-promise-interface": "^0.1.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "budo": "^8.0.2",
    "concat-stream": "^1.5.1",
    "isomorphic-fetch": "^2.2.0",
    "leaflet-transitivelayer": "^0.1.0",
    "lodash.reverse": "^4.0.1",
    "mapbox.js": "^2.2.4",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "standard": "^5.3.1",
    "tape": "^4.2.2",
    "transitive-js": "^0.8.2"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
